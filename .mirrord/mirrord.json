{
    "target": {
        "path": {
            "deployment": "cluster-api-provider-linode-controller-manager",
            "container": "manager"
        },
        "namespace": "cluster-api-provider-linode-system"
    },
    "agent": {
      "log_level": "info",
      "namespace": "cluster-api-provider-linode-system",
      "privileged": true
    },
    "feature": {
        "network": {
            "incoming": "steal",
            "outgoing": false
        },
        "fs": "local",
        "env": true
    },
    "operator": false,
    "pause": true,
    "accept_invalid_certificates": true
}
